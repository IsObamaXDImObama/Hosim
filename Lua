					local script = Instance.new('LocalScript', ProccedDescriptionButton_2)

					script.Parent.MouseButton1Click:Connect(function()
						script.Parent.Parent.Parent:TweenSize(UDim2.new(0, 740,0, 0),"InOut","Quad",0.3)
						wait(0.2)
